<span class="nd">@CsvSource</span><span class="o">({</span><span class="s">"10, 'ìë°” ìŠ¤í„°ë””'"</span><span class="o">,</span> <span class="s">"20, 'ìŠ¤í”„ë§'"</span><span class="o">})</span>
<span class="kt">void</span> <span class="nf">parameterizedTest</span><span class="o">(</span><span class="nc">ArgumentsAccessor</span> <span class="n">argumentsAccessor</span><span class="o">){</span>
    <span class="nc">Study</span> <span class="n">study</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">Study</span><span class="o">(</span><span class="n">argumentsAccessor</span><span class="o">.</span><span class="na">getInteger</span><span class="o">(</span><span class="mi">0</span><span class="o">),</span> <span class="n">argumentsAccessor</span><span class="o">.</span><span class="na">getString</span><span class="o">(</span><span class="mi">1</span><span class="o">));</span>
    <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">study</span><span class="o">);</span>
<span class="o">}</span>
</code></pre></div></div>

<ul>
  <li>ì»¤ìŠ¤í…€ Accessor
    <ul>
      <li>ArgumentsAggregator ì¸í„°í˜ì´ìŠ¤ êµ¬í˜„, ë°˜ë“œì‹œ static inner classì´ê±°ë‚˜ public class í˜•íƒœë¡œ ë§Œë“¤ì–´ì•¼ í•¨.</li>
      <li>@AggregateWith</li>
    </ul>

    <div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code>  <span class="nd">@DisplayName</span><span class="o">(</span><span class="s">"ìŠ¤í„°ë”” ë§Œë“¤ê¸°"</span><span class="o">)</span>
  <span class="nd">@ParameterizedTest</span><span class="o">(</span><span class="n">name</span> <span class="o">=</span> <span class="s">"{index} {displayName} message = {0}"</span><span class="o">)</span>
  <span class="nd">@CsvSource</span><span class="o">({</span><span class="s">"10, 'ìë°” ìŠ¤í„°ë””'"</span><span class="o">,</span> <span class="s">"20, 'ìŠ¤í”„ë§'"</span><span class="o">})</span>
  <span class="kt">void</span> <span class="nf">parameterizedTest</span><span class="o">(</span><span class="nd">@AggregateWith</span><span class="o">(</span><span class="nc">StudyAggregator</span><span class="o">.</span><span class="na">class</span><span class="o">)</span> <span class="nc">Study</span> <span class="n">study</span><span class="o">){</span>
      <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">study</span><span class="o">);</span>
  <span class="o">}</span>

  <span class="kd">static</span> <span class="kd">class</span> <span class="nc">StudyAggregator</span> <span class="kd">implements</span> <span class="nc">ArgumentsAggregator</span><span class="o">{</span>

      <span class="nd">@Override</span>
      <span class="kd">public</span> <span class="nc">Object</span> <span class="nf">aggregateArguments</span><span class="o">(</span><span class="nc">ArgumentsAccessor</span> <span class="n">argumentsAccessor</span><span class="o">,</span> <span class="nc">ParameterContext</span> <span class="n">parameterContext</span><span class="o">)</span> <span class="kd">throws</span> <span class="nc">ArgumentsAggregationException</span> <span class="o">{</span>
          <span class="k">return</span> <span class="k">new</span> <span class="nf">Study</span><span class="o">(</span><span class="n">argumentsAccessor</span><span class="o">.</span><span class="na">getInteger</span><span class="o">(</span><span class="mi">0</span><span class="o">),</span> <span class="n">argumentsAccessor</span><span class="o">.</span><span class="na">getString</span><span class="o">(</span><span class="mi">1</span><span class="o">));</span>
      <span class="o">}</span>
  <span class="o">}</span>
</code></pre></div>    </div>
  </li>
</ul>

