<span class="nd">@TestMethodOrder</span><span class="o">(</span><span class="nc">MethodOrderer</span><span class="o">.</span><span class="na">___________</span><span class="o">.</span><span class="na">class</span><span class="o">)</span>
<span class="kd">public</span> <span class="kd">class</span> <span class="nc">StudyCreateUsecaseTest</span> <span class="o">{</span>

    <span class="kd">private</span> <span class="nc">Study</span> <span class="n">study</span><span class="o">;</span>

    <span class="nd">@Order</span><span class="o">(</span><span class="mi">1</span><span class="o">)</span>
    <span class="nd">@Test</span>
    <span class="nd">@DisplayName</span><span class="o">(</span><span class="s">"ìŠ¤í„°ë”” ë§Œë“¤ê¸°"</span><span class="o">)</span>
    <span class="kd">public</span> <span class="kt">void</span> <span class="nf">create_study</span><span class="o">()</span> <span class="o">{</span>
        <span class="n">study</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">Study</span><span class="o">(</span><span class="mi">10</span><span class="o">,</span> <span class="s">"ìë°”"</span><span class="o">);</span>
        <span class="n">assertEquals</span><span class="o">(</span><span class="nc">StudyStatus</span><span class="o">.</span><span class="na">DRAFT</span><span class="o">,</span> <span class="n">study</span><span class="o">.</span><span class="na">getStatus</span><span class="o">());</span>
    <span class="o">}</span>

    <span class="nd">@Order</span><span class="o">(</span><span class="mi">2</span><span class="o">)</span>
    <span class="nd">@Test</span>
    <span class="nd">@DisplayName</span><span class="o">(</span><span class="s">"ìŠ¤í„°ë”” ê³µê°œ"</span><span class="o">)</span>
    <span class="kd">public</span> <span class="kt">void</span> <span class="nf">publish_study</span><span class="o">()</span> <span class="o">{</span>
        <span class="n">study</span><span class="o">.</span><span class="na">publish</span><span class="o">();</span>
        <span class="n">assertEquals</span><span class="o">(</span><span class="nc">StudyStatus</span><span class="o">.</span><span class="na">OPENED</span><span class="o">,</span> <span class="n">study</span><span class="o">.</span><span class="na">getStatus</span><span class="o">());</span>
