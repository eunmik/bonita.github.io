    <span class="kd">public</span> <span class="kd">static</span> <span class="kt">boolean</span> <span class="nf">next</span><span class="o">(</span><span class="kt">int</span><span class="o">[]</span> <span class="n">red</span><span class="o">,</span><span class="kt">int</span><span class="o">[]</span> <span class="n">blue</span><span class="o">,</span> <span class="kt">int</span> <span class="n">dir</span><span class="o">){</span>
